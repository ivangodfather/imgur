# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'imgur' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for imgur
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'Moya'
    pod 'Kingfisher'


  target 'imgurTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
    pod 'RxBlocking'
    pod 'OHHTTPStubs'
    pod 'OHHTTPStubs/Swift'
  end

  target 'imgurUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Nimble'
  end

end
